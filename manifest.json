{
    "manifest_version": 3,
    "name": "Alma Plus",
    "version": "1.2",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://ucla.alma.exlibrisgroup.com/*"
            ],
            "run_at": "document_end"
        }
    ]
}